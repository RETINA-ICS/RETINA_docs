        <mxCell id="TTrzwmC-LAXirWhwuJf--20" value="&lt;b&gt;workflows&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fontFamily=Fira Code;fontSize=12;" parent="1" vertex="1">
